#!/bin/bash

source .config

TIMEOUT=2500

mkdir -p $MATCHES_PATH
TMP_REPO=`mktemp -d $MATCHES_PATH/XXXXXXXXX` || exit -1
cd $TMP_REPO
git clone $MASTER_REPO .
mkdir rounds
which rrobots
CMD="rrobots --resolution 1600,900 --timeout $TIMEOUT bots/*.rb"
$CMD > rounds/round-x.1.txt
$CMD > rounds/round-x.2.txt
$CMD > rounds/round-x.3.txt
